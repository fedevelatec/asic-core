  <modelVersion>4.0.0</modelVersion>

  <groupId>com.fedevela.core</groupId>
  <artifactId>asic-core</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>asic-core</name>
  <url>http://maven.apache.org</url>

  <properties>
      <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
      <org.slf4j.version>1.6.1</org.slf4j.version>
      <hibernate.version>3.6.10.Final</hibernate.version>
      <hibernate.annotations.version>3.5.6-Final</hibernate.annotations.version>
      <org.codehaus.jackson.version>1.9.3</org.codehaus.jackson.version>
      <org.jdom.version>1.1.3</org.jdom.version>
      <org.apache.poi.version>3.9</org.apache.poi.version>
      <com.lowagie.itext.version>2.1.7</com.lowagie.itext.version>
  </properties>

  <dependencies>
      <dependency>
          <groupId>org.hibernate</groupId>
          <artifactId>hibernate-entitymanager</artifactId>
          <version>${hibernate.version}</version>
          <scope>compile</scope>
      </dependency>
      <dependency>
          <groupId>org.codehaus.jackson</groupId>
          <artifactId>jackson-mapper-asl</artifactId>
          <version>${org.codehaus.jackson.version}</version>
          <scope>compile</scope>
      </dependency>
      <dependency>
          <groupId>org.codehaus.jackson</groupId>
          <artifactId>jackson-core-asl</artifactId>
          <version>${org.codehaus.jackson.version}</version>
          <scope>compile</scope>
      </dependency>
      <dependency>
          <groupId>net.codicentro</groupId>
          <artifactId>codicentro-core</artifactId>
          <version>2.0.0</version>
          <scope>provided</scope>
      </dependency>
      <!-- Dependencias de codi -->
      <dependency>
          <groupId>org.jdom</groupId>
          <artifactId>jdom</artifactId>
          <version>${org.jdom.version}</version>
      </dependency>
      <dependency>
          <groupId>org.slf4j</groupId>
          <artifactId>slf4j-log4j12</artifactId>
          <version>${org.slf4j.version}</version>
          <type>jar</type>
      </dependency>
      <dependency>
          <groupId>org.apache.poi</groupId>
          <artifactId>poi</artifactId>
          <version>${org.apache.poi.version}</version>
          <scope>provided</scope>
          <exclusions>
              <exclusion>
                  <artifactId>commons-codec</artifactId>
                  <groupId>commons-codec</groupId>
              </exclusion>
          </exclusions>
      </dependency>
      <dependency>
          <groupId>org.apache.poi</groupId>
          <artifactId>poi-ooxml</artifactId>
          <version>${org.apache.poi.version}</version>
          <scope>provided</scope>
